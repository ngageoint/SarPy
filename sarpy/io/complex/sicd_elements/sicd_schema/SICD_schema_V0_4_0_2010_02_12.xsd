<!DOCTYPE HTML>
<!DOCTYPE html PUBLIC "" ""><HTML><HEAD>
<META http-equiv="Content-Type" content="text/html; charset=utf-8"></HEAD>
<BODY>
<PRE>&lt;?xml version="1.0" encoding="UTF-8"?&gt;
&lt;xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
   elementFormDefault="qualified"
   attributeFormDefault="unqualified"
   targetNamespace="urn:SICD:0.4.0"
   xmlns="urn:SICD:0.4.0"&gt;

&lt;!-- SICD TYPES --&gt;

   &lt;xs:complexType name="integertype"&gt;
      &lt;xs:simpleContent&gt;
         &lt;xs:extension base="xs:int"&gt;
            &lt;xs:attribute name="class" type="xs:string" use="required" fixed="xs:int"/&gt;
         &lt;/xs:extension&gt;
      &lt;/xs:simpleContent&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="doubletype"&gt;
      &lt;xs:simpleContent&gt;
         &lt;xs:extension base="xs:double"&gt;
            &lt;xs:attribute name="class" type="xs:string" use="required" fixed="xs:double"/&gt;
         &lt;/xs:extension&gt;
      &lt;/xs:simpleContent&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="stringtype"&gt;
      &lt;xs:simpleContent&gt;
         &lt;xs:extension base="xs:string"&gt;
            &lt;xs:attribute name="class" type="xs:string" use="required" fixed="xs:string"/&gt;
         &lt;/xs:extension&gt;
      &lt;/xs:simpleContent&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="booleantype"&gt;
      &lt;xs:simpleContent&gt;
         &lt;xs:extension base="xs:boolean"&gt;
            &lt;xs:attribute name="class" type="xs:string" use="required" fixed="xs:boolean"/&gt;
         &lt;/xs:extension&gt;
      &lt;/xs:simpleContent&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="dateTimetype"&gt;
      &lt;xs:simpleContent&gt;
         &lt;xs:extension base="xs:dateTime"&gt;
            &lt;xs:attribute name="class" type="xs:string" use="required" fixed="xs:dateTime"/&gt;
         &lt;/xs:extension&gt;
      &lt;/xs:simpleContent&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="xyztype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="X" type="doubletype" maxOccurs="1"/&gt;
         &lt;xs:element name="Y" type="doubletype" maxOccurs="1"/&gt;
         &lt;xs:element name="Z" type="doubletype" maxOccurs="1"/&gt;
      &lt;/xs:sequence&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="xyzAttributetype"&gt;
      &lt;xs:complexContent&gt;
         &lt;xs:extension base="xyztype"&gt;
            &lt;xs:attribute name="name" type="xs:string" use="optional"/&gt;
         &lt;/xs:extension&gt;
      &lt;/xs:complexContent&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="llhtype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="Lat" type="doubletype" maxOccurs="1"/&gt;
         &lt;xs:element name="Lon" type="doubletype" maxOccurs="1"/&gt;
         &lt;xs:element name="HAE" type="doubletype" maxOccurs="1"/&gt;
      &lt;/xs:sequence&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="latlontype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="Lat" type="doubletype" maxOccurs="1"/&gt;
         &lt;xs:element name="Lon" type="doubletype" maxOccurs="1"/&gt;
      &lt;/xs:sequence&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:simpleType name="cornerattrtype"&gt;
      &lt;xs:restriction base="xs:int"&gt;
         &lt;xs:minInclusive value="1"/&gt;
         &lt;xs:maxInclusive value="4"/&gt;
      &lt;/xs:restriction&gt;
   &lt;/xs:simpleType&gt;

   &lt;xs:simpleType name="cornerstringtype"&gt;
      &lt;xs:restriction base="xs:string"&gt;
         &lt;xs:enumeration value="1:FRFC"/&gt;
         &lt;xs:enumeration value="2:FRLC"/&gt;
         &lt;xs:enumeration value="3:LRLC"/&gt;
         &lt;xs:enumeration value="4:LRFC"/&gt;
      &lt;/xs:restriction&gt;
   &lt;/xs:simpleType&gt;

   &lt;xs:complexType name="latloncornertype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="Lat" type="doubletype" maxOccurs="1"/&gt;
         &lt;xs:element name="Lon" type="doubletype" maxOccurs="1"/&gt;
      &lt;/xs:sequence&gt;
      &lt;xs:attribute name="index" type="cornerattrtype" use="required"/&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="latloncornerstringtype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="Lat" type="doubletype" maxOccurs="1"/&gt;
         &lt;xs:element name="Lon" type="doubletype" maxOccurs="1"/&gt;
      &lt;/xs:sequence&gt;
      &lt;xs:attribute name="index" type="cornerstringtype" use="required"/&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="latlonhaecornertype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="Lat" type="doubletype" maxOccurs="1"/&gt;
         &lt;xs:element name="Lon" type="doubletype" maxOccurs="1"/&gt;
         &lt;xs:element name="HAE" type="doubletype" maxOccurs="1"/&gt;
      &lt;/xs:sequence&gt;
      &lt;xs:attribute name="index" type="cornerattrtype" use="required"/&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="llhcornerstringtype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="Lat" type="doubletype" maxOccurs="1"/&gt;
         &lt;xs:element name="Lon" type="doubletype" maxOccurs="1"/&gt;
         &lt;xs:element name="HAE" type="doubletype" maxOccurs="1"/&gt;
      &lt;/xs:sequence&gt;
      &lt;xs:attribute name="index" type="cornerstringtype" use="required"/&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="rowcoltype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="Row" type="integertype" maxOccurs="1"/&gt;
         &lt;xs:element name="Col" type="integertype" maxOccurs="1"/&gt;
      &lt;/xs:sequence&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="complextype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="Real" type="doubletype" maxOccurs="1"/&gt;
         &lt;xs:element name="Imag" type="doubletype" maxOccurs="1"/&gt;
      &lt;/xs:sequence&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="rowcolvertextype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="Row" type="integertype" maxOccurs="1"/&gt;
         &lt;xs:element name="Col" type="integertype" maxOccurs="1"/&gt;
      &lt;/xs:sequence&gt;
      &lt;xs:attribute name="index" type="xs:int" use="required"/&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="polycoef1dtype"&gt;
      &lt;xs:simpleContent&gt;
         &lt;xs:extension base="doubletype"&gt;
            &lt;xs:attribute name="exponent1" type="xs:int" use="required"/&gt;
         &lt;/xs:extension&gt;
      &lt;/xs:simpleContent&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="polycoef2dtype"&gt;
      &lt;xs:simpleContent&gt;
         &lt;xs:extension base="doubletype"&gt;
            &lt;xs:attribute name="exponent1" type="xs:int" use="required"/&gt;
            &lt;xs:attribute name="exponent2" type="xs:int" use="required"/&gt;
         &lt;/xs:extension&gt;
      &lt;/xs:simpleContent&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="poly1dtype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="Coef" type="polycoef1dtype" minOccurs="1" maxOccurs="unbounded"/&gt;
      &lt;/xs:sequence&gt;
      &lt;xs:attribute name="order1" type="xs:int" use="required"/&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="poly2dtype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="Coef" type="polycoef2dtype" minOccurs="1" maxOccurs="unbounded"/&gt;
      &lt;/xs:sequence&gt;
      &lt;xs:attribute name="order1" type="xs:int" use="required"/&gt;
      &lt;xs:attribute name="order2" type="xs:int" use="required"/&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="xyzpolytype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="X" type="poly1dtype" maxOccurs="1"/&gt;
         &lt;xs:element name="Y" type="poly1dtype" maxOccurs="1"/&gt;
         &lt;xs:element name="Z" type="poly1dtype" maxOccurs="1"/&gt;
      &lt;/xs:sequence&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="xyzpolyAttributetype"&gt;
      &lt;xs:complexContent&gt;
         &lt;xs:extension base="xyzpolytype"&gt;
            &lt;xs:attribute name="index" type="xs:int" use="required"/&gt;
         &lt;/xs:extension&gt;
      &lt;/xs:complexContent&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="linetype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="Endpoint" minOccurs="2" maxOccurs="unbounded"&gt;
            &lt;xs:complexType&gt;
               &lt;xs:sequence&gt;
                  &lt;xs:element name="Lat" type="doubletype" maxOccurs="1"/&gt;
                  &lt;xs:element name="Lon" type="doubletype" maxOccurs="1"/&gt;
               &lt;/xs:sequence&gt;
               &lt;xs:attribute name="index" type="xs:int" use="required"/&gt;
            &lt;/xs:complexType&gt;
         &lt;/xs:element&gt;
      &lt;/xs:sequence&gt;
      &lt;xs:attribute name="size" type="xs:int" use="required"/&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="polygontype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="Vertex" minOccurs="3" maxOccurs="unbounded"&gt;
            &lt;xs:complexType&gt;
               &lt;xs:sequence&gt;
                  &lt;xs:element name="Lat" type="doubletype" maxOccurs="1"/&gt;
                  &lt;xs:element name="Lon" type="doubletype" maxOccurs="1"/&gt;
               &lt;/xs:sequence&gt;
               &lt;xs:attribute name="index" type="xs:int" use="required"/&gt;
            &lt;/xs:complexType&gt;
         &lt;/xs:element&gt;
      &lt;/xs:sequence&gt;
      &lt;xs:attribute name="size" type="xs:int" use="required"/&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="arraydoubtype"&gt;
      &lt;xs:simpleContent&gt;
         &lt;xs:extension base="doubletype"&gt;
            &lt;xs:attribute name="index" type="xs:int" use="required"/&gt;
         &lt;/xs:extension&gt;
      &lt;/xs:simpleContent&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="parametertype"&gt;
      &lt;xs:simpleContent&gt;
         &lt;xs:extension base="stringtype"&gt;
            &lt;xs:attribute name="name" type="xs:string" use="required"/&gt;
         &lt;/xs:extension&gt;
      &lt;/xs:simpleContent&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="orientationtype"&gt;
      &lt;xs:simpleContent&gt;
         &lt;xs:restriction base="stringtype"&gt;
            &lt;xs:enumeration value="UP"/&gt;
            &lt;xs:enumeration value="DOWN"/&gt;
            &lt;xs:enumeration value="LEFT"/&gt;
            &lt;xs:enumeration value="RIGHT"/&gt;
            &lt;xs:enumeration value="ARBITRARY"/&gt;
         &lt;/xs:restriction&gt;
      &lt;/xs:simpleContent&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="polarizationtype"&gt;
      &lt;xs:simpleContent&gt;
         &lt;xs:restriction base="stringtype"&gt;
            &lt;xs:enumeration value="V"/&gt;
            &lt;xs:enumeration value="H"/&gt;
            &lt;xs:enumeration value="RHC"/&gt;
            &lt;xs:enumeration value="LHC"/&gt;
            &lt;xs:enumeration value="OTHER"/&gt;
         &lt;/xs:restriction&gt;
      &lt;/xs:simpleContent&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="dualpolarizationtype"&gt;
      &lt;xs:simpleContent&gt;
         &lt;xs:restriction base="stringtype"&gt;
            &lt;xs:enumeration value="V:V"/&gt;
            &lt;xs:enumeration value="V:H"/&gt;
            &lt;xs:enumeration value="H:V"/&gt;
            &lt;xs:enumeration value="H:H"/&gt;
            &lt;xs:enumeration value="RHC:RHC"/&gt;
            &lt;xs:enumeration value="RHC:LHC"/&gt;
            &lt;xs:enumeration value="LHC:RHC"/&gt;
            &lt;xs:enumeration value="LHC:LHC"/&gt;
            &lt;xs:enumeration value="OTHER"/&gt;
         &lt;/xs:restriction&gt;
      &lt;/xs:simpleContent&gt;
   &lt;/xs:complexType&gt;



   &lt;xs:complexType name="zeroto90type"&gt;
      &lt;xs:simpleContent&gt;
         &lt;xs:restriction base="doubletype"&gt;
            &lt;xs:minInclusive value="0"/&gt;
            &lt;xs:maxInclusive value="90"/&gt;
         &lt;/xs:restriction&gt;
      &lt;/xs:simpleContent&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="neg90to90type"&gt;
      &lt;xs:simpleContent&gt;
         &lt;xs:restriction base="doubletype"&gt;
            &lt;xs:minInclusive value="-90"/&gt;
            &lt;xs:maxInclusive value="90"/&gt;
         &lt;/xs:restriction&gt;
      &lt;/xs:simpleContent&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="neg180to180type"&gt;
      &lt;xs:simpleContent&gt;
         &lt;xs:restriction base="doubletype"&gt;
            &lt;xs:minInclusive value="-180"/&gt;
            &lt;xs:maxInclusive value="180"/&gt;
         &lt;/xs:restriction&gt;
      &lt;/xs:simpleContent&gt;
   &lt;/xs:complexType&gt;


&lt;!-- sicd blocks --&gt;

   &lt;xs:complexType name="collectioninfotype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="CollectorName" type="stringtype" maxOccurs="1"/&gt;
         &lt;xs:element name="IlluminatorName" type="stringtype" minOccurs="0" maxOccurs="1"/&gt;
         &lt;xs:element name="CoreName" type="stringtype" maxOccurs="1"/&gt;
         &lt;xs:element name="CollectType" minOccurs="0" maxOccurs="1" &gt;
            &lt;xs:complexType&gt;
               &lt;xs:simpleContent&gt;
                  &lt;xs:restriction base="stringtype"&gt;
                     &lt;xs:enumeration value="MONOSTATIC"/&gt;
                     &lt;xs:enumeration value="BISTATIC"/&gt;
                  &lt;/xs:restriction&gt;
               &lt;/xs:simpleContent&gt;
            &lt;/xs:complexType&gt;
         &lt;/xs:element&gt;
         &lt;xs:element name="RadarMode" maxOccurs="1"&gt;
            &lt;xs:complexType&gt;
               &lt;xs:sequence&gt;
                  &lt;xs:element name="ModeType" maxOccurs="1"&gt;
                     &lt;xs:complexType&gt;
                        &lt;xs:simpleContent&gt;
                           &lt;xs:restriction base="stringtype"&gt;
                              &lt;xs:enumeration value="SPOTLIGHT"/&gt;
                              &lt;xs:enumeration value="STRIPMAP"/&gt;
                              &lt;xs:enumeration value="DYNAMIC STRIPMAP"/&gt;
                           &lt;/xs:restriction&gt;
                        &lt;/xs:simpleContent&gt;
                     &lt;/xs:complexType&gt;
                  &lt;/xs:element&gt;
                  &lt;xs:element name="ModeID" type="stringtype" minOccurs="0" maxOccurs="1"/&gt;
               &lt;/xs:sequence&gt;
            &lt;/xs:complexType&gt;
         &lt;/xs:element&gt;
         &lt;xs:element name="Classification" type="stringtype" maxOccurs="1"/&gt;
         &lt;xs:element name="CountryCode" type="stringtype" minOccurs="0" maxOccurs="unbounded"/&gt;
         &lt;xs:element name="Parameter" type="parametertype" minOccurs="0" maxOccurs="unbounded"/&gt;
      &lt;/xs:sequence&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="imagecreationtype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="Application" type="stringtype" minOccurs="0" maxOccurs="1"/&gt;
         &lt;xs:element name="DateTime" type="dateTimetype" minOccurs="0" maxOccurs="1"/&gt;
         &lt;xs:element name="Site" type="stringtype" minOccurs="0" maxOccurs="1"/&gt;
         &lt;xs:element name="Profile" type="stringtype" minOccurs="0" maxOccurs="1"/&gt;
      &lt;/xs:sequence&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="imagedatatype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="PixelType" maxOccurs="1"&gt;
            &lt;xs:complexType&gt;
               &lt;xs:simpleContent&gt;
                  &lt;xs:restriction base="stringtype"&gt;
                     &lt;xs:enumeration value="RE32F_IM32F"/&gt;
                     &lt;xs:enumeration value="RE16I_IM16I"/&gt;
                     &lt;xs:enumeration value="AMP8I_PHS8I"/&gt;
                  &lt;/xs:restriction&gt;
               &lt;/xs:simpleContent&gt;
            &lt;/xs:complexType&gt;
         &lt;/xs:element&gt;
         &lt;xs:element name="AmpTable" minOccurs="0" maxOccurs="1"&gt;
            &lt;xs:complexType&gt;
               &lt;xs:sequence&gt;
                  &lt;xs:element name="Amplitude" type="arraydoubtype" minOccurs="1" maxOccurs="256"/&gt;
               &lt;/xs:sequence&gt;
               &lt;xs:attribute name="size" type="xs:int" use="required"/&gt;
            &lt;/xs:complexType&gt;
         &lt;/xs:element&gt;
         &lt;xs:element name="NumRows" type="integertype" maxOccurs="1"/&gt;
         &lt;xs:element name="NumCols" type="integertype" maxOccurs="1"/&gt;
         &lt;xs:element name="FirstRow" type="integertype" maxOccurs="1"/&gt;
         &lt;xs:element name="FirstCol" type="integertype" maxOccurs="1"/&gt;
         &lt;xs:element name="FullImage" maxOccurs="1"&gt;
            &lt;xs:complexType&gt;
               &lt;xs:sequence&gt;
                  &lt;xs:element name="NumRows" type="integertype" maxOccurs="1"/&gt;
                  &lt;xs:element name="NumCols" type="integertype" maxOccurs="1"/&gt;
               &lt;/xs:sequence&gt;
            &lt;/xs:complexType&gt;
         &lt;/xs:element&gt;
         &lt;xs:element name="SCPPixel" type="rowcoltype" maxOccurs="1"/&gt;
         &lt;xs:element name="ValidData" minOccurs="0" maxOccurs="1" &gt;
            &lt;xs:complexType&gt;
               &lt;xs:sequence&gt;
                  &lt;xs:element name="Vertex" type="rowcolvertextype" minOccurs="3" maxOccurs="unbounded"/&gt;
               &lt;/xs:sequence&gt;
               &lt;xs:attribute name="size" type="xs:int" use="required"/&gt;
            &lt;/xs:complexType&gt;
         &lt;/xs:element&gt;
      &lt;/xs:sequence&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="geoinfotype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="GeoInfo" type="geoinfotype" minOccurs="0" maxOccurs="unbounded"/&gt;
         &lt;xs:element name="Desc" type="parametertype" minOccurs="0" maxOccurs="unbounded"/&gt;
         &lt;xs:choice minOccurs="0" maxOccurs="1"&gt;
            &lt;xs:element name="Point" type="latlontype"/&gt;
            &lt;xs:element name="Line" type="linetype"/&gt;
            &lt;xs:element name="Polygon" type="polygontype"/&gt;
         &lt;/xs:choice&gt;
      &lt;/xs:sequence&gt;
      &lt;xs:attribute name="name" type="xs:string" use="required"/&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="geodatatype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="EarthModel" maxOccurs="1"&gt;
            &lt;xs:complexType&gt;
               &lt;xs:simpleContent&gt;
                  &lt;xs:restriction base="stringtype"&gt;
                     &lt;xs:enumeration value="WGS_84"/&gt;
                  &lt;/xs:restriction&gt;
               &lt;/xs:simpleContent&gt;
            &lt;/xs:complexType&gt;
         &lt;/xs:element&gt;
         &lt;xs:element name="SCP" maxOccurs="1"&gt;
            &lt;xs:complexType&gt;
               &lt;xs:sequence&gt;
                  &lt;xs:element name="ECF" type="xyztype" maxOccurs="1"/&gt;
                  &lt;xs:element name="LLH" type="llhtype" maxOccurs="1"/&gt;
               &lt;/xs:sequence&gt;
            &lt;/xs:complexType&gt;
         &lt;/xs:element&gt;
         &lt;xs:element name="ImageCorners" maxOccurs="1"&gt;
            &lt;xs:complexType&gt;
               &lt;xs:sequence&gt;
                  &lt;xs:element name="ICP" type="latloncornerstringtype" minOccurs="4" maxOccurs="4"/&gt;
               &lt;/xs:sequence&gt;
            &lt;/xs:complexType&gt;
         &lt;/xs:element&gt;
         &lt;xs:element name="ValidData" type="polygontype" minOccurs="0" maxOccurs="1"/&gt;
         &lt;xs:element name="GeoInfo" type="geoinfotype" minOccurs="0" maxOccurs="unbounded"/&gt;
      &lt;/xs:sequence&gt;
   &lt;/xs:complexType&gt;

   &lt;xs:complexType name="dirparamtype"&gt;
      &lt;xs:sequence&gt;
         &lt;xs:element name="UVectECF" type="xyztype" maxOccurs="1"/&gt;
         &lt;xs:element name="SS" type="doubletype" maxOccurs="1"/&gt;
         &lt;xs:element name="ImpRespWid" type="doubletype" maxOccurs="1"/&gt;
         &lt;xs:element name="Sgn" maxOccurs="1"&gt;
            &lt;xs:complexType&gt;
               &lt;xs:simpleContent&gt;
                  &lt;xs:restriction base="integertype"&gt;
                     &lt;xs:enumeration value="+1"/&gt;
                     &lt;xs:enumeration value="-1"/&gt;
                  &lt;/xs:restriction&gt;
               &lt;/xs:simpleContent&gt;
            &lt;/xs:complexType&gt;
         &lt;/xs:element&gt;
         &lt;xs:element name="ImpRespBW" type="doubletype" maxOccurs="1"/&gt;
         &lt;xs:element name="KCtr" type="doubletype" maxOccurs="1"/&gt;
         &lt;xs:element name="DeltaK1" type="doubletype" maxOccurs="1"/&gt;
         &lt;xs:element name="DeltaK2" type="doubletype" maxOccurs="1"/&gt;
         &lt;xs:element name="DeltaKCOAPoly" type="poly2dtype" minOccurs="0" maxOccurs="1"/&gt;
         &lt;xs:element name="WgtType" type="stringtype" minOccurs="0" maxOccurs="1"/&gt;
         &lt;xs:element name="WgtFunct" minOccurs="0" maxOccurs="1"&gt;
            &lt;xs:complexType&gt;
               &lt;xs:sequence&gt;
                  &lt;xs:element name="Wgt" type="arraydoubtype" minOccurs="2" maxOccurs="512"/&gt;
               &lt;/xs:sequence&gt;
               &lt;xs:attribute name="size" type="xs:int" use="required"/&gt;
            &lt;/xs:complexType&gt;
